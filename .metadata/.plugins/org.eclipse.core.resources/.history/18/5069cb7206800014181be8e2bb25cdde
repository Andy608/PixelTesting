package graphics;

import java.util.Random;

public class PixelDisplay extends PixelManager {

	private PixelManager test;
	
	public PixelDisplay(int w, int h) {
		super(w, h);
		
		Random rand = new Random();
		test = new PixelManager(50, 50);
		
		for (int i = 0; i < test.width * test.height; i++) {
			test.pixels[i] = rand.nextInt();
		}
	}
	
	public void render(Game game) {
		
	}
	
}
