package graphics;

import javax.swing.JFrame;

public class PixelFrame extends JFrame{

	private static final long serialVersionUID = 7195384405114123587L;
	private static PixelCanavas pc = new PixelCanvas();
	
	public PixelFrame() {
		this.setTitle("Pixel Testing!");
		this.add(pc);
		this.setResizable(false);
		this.setMinimumSize(anvas.getMinimumSize());
		this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}
	
	
	
}
